//question 1

abstract class Shape {
    abstract void area();
}
public class Circle extends Shape{
    double r = 3.0;
    void area(){
        System.out.println("Area : "+3.14*r*r);
    }
    
}

public class Rectangle extends Shape{
    double l = 4.0;
    double b = 3.0;
    void area(){
        System.out.println("Area : "+l*b);
    }
}


public class Main {
    public static void main(String[] args) {
        Circle c = new Circle();
        Rectangle r = new Rectangle();
        c.area();
        r.area();
    }
    
}
//question 2

public class Shape {
    void display(){
        System.out.println("This is a shape.");
    }
}

public class Circle extends Shape{
    void display(){
        super.display();
        System.out.println("This a Circle.");
    }
}

public class Rectangle extends Shape{
    void display(){
        super.display();
        System.out.println("This a Rectangle.");
    }
}


public class Main {
    public static void main(String[] args) {
        Circle c = new Circle();
        Rectangle r = new Rectangle();
        c.display();
        r.display();
    }
}

\\question 3
public class Shape {
    void area(){
        System.out.println("It is a shape");
    }
}

public class Circle extends Shape{
    int r = 5;
    void area(){
        System.out.println("Area of circle :"+ (3.14*r*r));
    }
}
public class Rectangle extends Shape{
    int l = 5;
    int b = 4;
    void area(){
        System.out.println("Area of Rectangle :"+ l*b);
    }
}
public class Main {
    public static void main(String[] args) {
        Shape c = new Circle();
        c.area();
        Shape r = new Rectangle();
        r.area();
    }
}

